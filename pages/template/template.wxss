/**template.wxss**/
.pageHead {
  display: flex;
  justify-content: space-between;
  height: 88rpx;
  line-height: 88rpx;
  border-bottom: 2rpx solid #eeeeee;
  padding: 0 2%;
  margin-bottom: 10rpx;
}

.pageEmpty{
  display: flex;
  flex-direction: column;
  align-items: center;
  height: calc(100vh - 88rpx);
  justify-content: center;
  color: #eeeeee; 
}

.grayBlock {
  background: #eeeeee;
}


